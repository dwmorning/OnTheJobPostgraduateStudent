page {
    text-rendering: optimizelegibility;
    color: #333;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    font-size: 26rpx;
    margin: 0;
    padding: 0;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.bgf8 {
    background-color: #f8f8f8;
}

.flex {
    display: flex;
}

.bgfff {
    background-color: #fff;
}

.bbso {
    border-bottom: 1rpx solid #eee;
}

.p10 {
    padding: 0 30rpx;
}

.wpTit {
    align-items: center;
    border-bottom: 1px solid #eaeaea;
    display: flex;
    justify-content: space-between;
    padding-bottom: 20rpx;
}

.wpTit text {
    font-size: 36rpx;
    font-weight: 700;
}

.wpTit .more {
    color: #999;
    font-size: 30rpx;
}

.nocontent {
    color: #999;
    display: flex;
    font-size: 28rpx;
    justify-content: center;
    padding: 50rpx 0;
}

.nocontent image {
    height: 348rpx;
    width: 356rpx;
}

.nocontent .loading {
    height: 59rpx;
    width: 118rpx;
}

::-webkit-scrollbar {
    color: transparent;
    height: 0;
    width: 0;
}

.clearfix:after {
    clear: both;
    content: "";
    display: block;
    font-size: 0;
    height: 0;
    visibility: hidden;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.backIndex {
    background: #4c6ff1;
    border-radius: 50%;
    bottom: 200rpx;
    box-sizing: border-box;
    color: #fff;
    font-size: 26rpx;
    height: 100rpx;
    padding: 0 15rpx;
    text-align: center;
    width: 100rpx;
}

.backIndex,.kefu {
    align-items: center;
    display: flex;
    justify-content: center;
    position: fixed;
    right: 40rpx;
}

.kefu {
    background-color: initial;
    background-image: linear-gradient(-90deg,#354cff,#5e7dff);
    border-radius: 80rpx;
    bottom: 32rpx;
    box-shadow: 0 0 40px 0 rgba(38,93,240,.3);
    height: 80rpx;
    padding: 0;
    width: 180rpx;
}

.kefu-txt {
    color: #fff;
    font-size: 15px;
    margin-left: 10rpx;
}

.kefu-img {
    height: 40rpx;
    width: 40rpx;
}

.jzCont .list {
    border-bottom: 1px solid #ebebeb;
    display: flex;
    justify-content: space-between;
    padding: 25rpx 0;
}

.jzCont .left {
    margin-right: 30rpx;
    width: 100rpx;
}

.jzCont .left image {
    height: 100rpx;
    width: 100rpx;
}

.jzCont .right {
    width: 560rpx;
}

.jzCont .tit {
    font-size: 32rpx;
    font-weight: 700;
    padding-right: 100rpx;
    position: relative;
}

.jzCont .pri {
    color: #be4039;
    font-size: 28rpx;
    font-weight: bolder;
    position: absolute;
    right: 0;
    top: 0;
}

.jzCont .sub {
    color: #666;
    font-size: 24rpx;
    margin-top: 14rpx;
}

.jzCont .tag text {
    border: 1px solid #cdcdcd;
    border-radius: 32rpx;
    color: #999;
    display: inline-block;
    font-size: 24rpx;
    height: 32rpx;
    line-height: 32rpx;
    margin-right: 10rpx;
    margin-top: 10rpx;
    overflow: hidden;
    padding: 2px 8px;
}

.jzCont .tag text:first-child {
    border-color: #ffb44c;
    color: #ffb44c;
    max-width: 180rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.jzCont .tag text:nth-child(3) {
    border-color: #90a2f5;
    color: #90a2f5;
    max-width: 110rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.jzCont .tag text:last-child {
    margin-right: 0;
}

.jzCont .linee:last-child .list {
    border-bottom: none;
}

.bottomFixed .apply {
    padding-left: 0;
    padding-right: 0;
}

.moress {
    background-color: #f7f8f9;
    border-radius: 4px;
    color: #999cab;
    font-size: 30rpx;
    line-height: 88rpx;
    margin: 0 30rpx 40rpx;
    text-align: center;
}